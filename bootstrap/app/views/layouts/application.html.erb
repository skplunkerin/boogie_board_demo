<!DOCTYPE html>
<html>
<head>
  <title>Freelancer - A Free Start Bootstrap Theme</title>
  <!-- Bootstrap Core CSS -->
  <%= stylesheet_link_tag    'bootstrap.min', media: 'all', 'data-turbolinks-track' => true %>
  <%= stylesheet_link_tag    'freelancer', media: 'all', 'data-turbolinks-track' => true %>

  <!-- Fonts -->
  <%= stylesheet_link_tag    'font-awesome/css/font-awesome.min', media: 'all', 'data-turbolinks-track' => true %>
  <link href='http://fonts.googleapis.com/css?family=Montserrat:400,700' rel='stylesheet' type='text/css'>

  <!-- IE8 support for HTML5 elements and media queries --> 
  <!--[if lt IE 9]> 
    <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
    <script src="https://oss.maxcdn.com/libs/respond.js/1.3.0/respond.min.js"></script>
  <![endif]-->
  <%= csrf_meta_tags %>
</head>
<body id="page-top" class="index">

<%= render :partial => 'layouts/nav' %>
<%= yield %>

<%= javascript_include_tag 'jquery-1.10.2', 'data-turbolinks-track' => true %>
<%= javascript_include_tag 'bootstrap.min', 'data-turbolinks-track' => true %>
<script src="http://cdnjs.cloudflare.com/ajax/libs/jquery-easing/1.3/jquery.easing.min.js"></script>
<%= javascript_include_tag 'classie', 'data-turbolinks-track' => true %>
<%= javascript_include_tag 'cbpAnimatedHeader', 'data-turbolinks-track' => true %>
<%= javascript_include_tag 'freelancer', 'data-turbolinks-track' => true %>
  
</body>
</html>
